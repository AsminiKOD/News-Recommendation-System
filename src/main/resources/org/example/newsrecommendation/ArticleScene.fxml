<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="455.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.newsrecommendation.App.ArticleScene">
   <children>
      <HBox layoutX="2.0" layoutY="40.0" prefHeight="100.0" prefWidth="409.0">
         <children>
            <Label fx:id="label_Heading" prefHeight="105.0" prefWidth="419.0" text="Heading" wrapText="true">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <ScrollPane layoutX="1.0" layoutY="255.0" prefHeight="200.0" prefWidth="411.0">
         <content>
            <Label fx:id="label_Article" alignment="TOP_LEFT" prefHeight="191.0" prefWidth="403.0" text="Content" wrapText="true" />
         </content>
      </ScrollPane>
      <Label fx:id="label_Category" layoutX="25.0" layoutY="167.0" text="Category" />
      <Label fx:id="label_Date" layoutX="328.0" layoutY="167.0" text="date" />
      <Button fx:id="button_Like" layoutX="218.0" layoutY="215.0" mnemonicParsing="false" onAction="#handleLike" text="Like" />
      <Button fx:id="button_Dislike" layoutX="290.0" layoutY="215.0" mnemonicParsing="false" onAction="#handleDislike" text="Dislike" />
      <Button fx:id="button_Save" layoutX="47.0" layoutY="215.0" mnemonicParsing="false" onAction="#handleSave" text="Save" />
   </children>
</AnchorPane>
