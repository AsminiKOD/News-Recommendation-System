<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="406.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.newsrecommendation.App.ArticleScene">
   <children>
      <HBox layoutX="19.0" layoutY="39.0" prefHeight="39.0" prefWidth="355.0">
         <children>
            <Label fx:id="label_Heading" prefHeight="43.0" prefWidth="355.0" text="Heading" wrapText="true">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <ScrollPane layoutX="14.0" layoutY="108.0" prefHeight="239.0" prefWidth="384.0">
         <content>
            <Label fx:id="label_Article" alignment="TOP_LEFT" prefHeight="238.0" prefWidth="381.0" text="Content" wrapText="true" />
         </content>
      </ScrollPane>
      <Label fx:id="label_Category" layoutX="20.0" layoutY="81.0" text="Category" />
      <Label fx:id="label_Date" layoutX="361.0" layoutY="78.0" text="date" />
      <Button fx:id="button_Like" layoutX="21.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleLike" text="Like" />
      <Button fx:id="button_Dislike" layoutX="347.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleDislike" text="Dislike" />
      <Button fx:id="button_Save" layoutX="348.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleSave" text="Save" />
   </children>
</AnchorPane>
